{
  "name": "cherrera-limp",
  "version": "4.8.1",
  "author": "Carlos Herrera",
  "summary": "Puppet module for create Ubuntu/Nginx/PHP/Mysql",
  "license": "Apache-2.0",
  "source": "https://github.com/caherrera/puppet-limp.git",
